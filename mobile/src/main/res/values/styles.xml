<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="SwitchCompatStyle" >
        <item name="thumbTint">@color/colorAccent</item>
        <item name="trackTint">@color/colorPrimaryDark</item>
    </style>

    <style name="Theme.AppTheme.Light.NoActionBar.Fullscreen" parent="android:Theme.Holo.Light.NoActionBar.Fullscreen" />


</resources>
